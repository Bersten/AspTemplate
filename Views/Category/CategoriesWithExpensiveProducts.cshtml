@model IEnumerable<Exercice.Models.Category>

<h1>Catégories avec des Produits Coûteux</h1>

<table>
    <thead>
        <tr>
            <th>Nom de la Catégorie</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.Name</td>
            </tr>
        }
    </tbody>
</table>